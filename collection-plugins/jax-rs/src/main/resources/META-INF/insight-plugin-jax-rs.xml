<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="https://www.springframework.org/schema/beans"
    xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
    xmlns:insight="https://www.springframework.org/schema/insight-idk"
    xsi:schemaLocation="https://www.springframework.org/schema/beans https://www.springframework.org/schema/beans/spring-beans.xsd
        https://www.springframework.org/schema/insight-idk https://www.springframework.org/schema/insight-idk/insight-idk-1.0.xsd">

    <insight:plugin name="jax-rs" version="${project.version}" publisher="SpringSource" />
    <insight:operation-view operation="jax-rs" template="com/springsource/insight/plugin/jaxrs/jax-rs.ftl"/>

    <bean id="jaxrsPluginEndPointAnalyzer"
          class="com.springsource.insight.plugin.jaxrs.JaxrsEndPointAnalyzer"
          factory-method="getInstance"
          lazy-init="true"
        />
    <bean id="jaxrsPluginExternalResourceAnalyzer"
          class="com.springsource.insight.plugin.jaxrs.JaxrsExternalResourceAnalyzer"
          factory-method="getInstance"
          lazy-init="true"
        />
    <bean id="jaxrsPluginRuntimeDescriptor"
          class="com.springsource.insight.plugin.jaxrs.JaxrsPluginRuntimeDescriptor"
          factory-method="getInstance"
          lazy-init="true"
        />
</beans>
